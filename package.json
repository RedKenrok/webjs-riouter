    "CHANGELOG.md",
    "README.md",
    "Router.js"
  ],
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "build:example": "NODE_ENV=production rollup -c ./example/rollup.config.js",
    "dev": "yarn run develop",
    "dev:example": "yarn run develop:example",
